        "@types/bcrypt": "^3.0.0",
        "@types/bluebird": "^3.5.28",
        "@types/config": "^0.0.34",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.1",
        "@types/jsonwebtoken": "^8.3.5",
        "@types/node": "^12.11.1",
