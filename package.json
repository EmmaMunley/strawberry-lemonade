        "@types/slonik": "^21.4.0",
        "@types/uuid": "^3.4.6",
        "@types/yargs": "^15.0.0",
        "aws-sdk": "^2.562.0",
        "axios": "^0.19.2",
        "bcrypt": "^3.0.6",
        "bcryptjs": "^2.4.3",
        "camelcase": "^5.3.1",
        "cheerio": "^1.0.0-rc.3",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.10.2",
        "cookie-parser": "^1.4.4",
        "date-fns": "^2.9.0",
        "express": "^4.17.1",
        "express-winston": "^4.0.1",
        "fp-ts": "^2.2.0",
        "inversify": "^5.0.1",
        "io-ts": "^2.0.1",
